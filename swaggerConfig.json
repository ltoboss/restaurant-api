{
    "swagger": {
        "outputDirectory": "./src/API/Config/",
        "entryFile": "./src/index.ts",
        "host": "localhost:3000",
        "version": "0.0.0",
        "name": "API Restaurant",
        "description": "API Restaurant",
        "basePath": "/api",
	 "apis": [
 	   "./src/API/Routes/**/*"
 	 ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "access_token",
                "in": "query"
            }
        }
    }
}
