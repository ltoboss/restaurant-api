{
  "name": "@api/restaurant",
  "version": "0.0.1",
  "description": "API Para la app de restaurante",
  "author": 
    "Luis Felipe Osuna <luisfelipeosuna@outlook.com>",
  "main": "lib/src/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "mocha": {
    "recursive": true,
    "extension": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "start": "ts-node .",
    "tsc": "tsc",
    "build": "yarn run tsc",
    "dev": "nodemon --exec yarn run dev:start",
    "dev:start": "yarn run build && yarn run start",
    "compile": "tsc -p .",
    "entity-create": "typeorm entity:create -n",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "sync": "yarn run ts-node lib/src/bin/sync.js",
    "sync-entities": "yarn sync sync-entities && yarn migration-global:create && yarn run ts-node lib/src/bin/sync-entities-success.js",
    "sync-entities-clear": "yarn clear && yarn build && yarn run ts-node lib/src/bin/sync-entities-clear.js"
  },
  "dependencies": {
    "@krack/utils": "git+ssh://git@github.com:ltoboss/utils.git#latest",
    "@types/google-libphonenumber": "^7.4.18",
    "@types/lodash": "^4.14.150",
    "amqplib": "^0.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "deasync-promise": "^1.0.1",
    "dotenv": "^6.2.0",
    "exceljs": "^2.0.1",
    "express": "^4.17.1",
    "express-get-endpoints": "^4.0.2",
    "hydra-express": "^1.7.1",
    "is-base64": "^0.1.0",
    "joi": "^14.3.1",
    "libphonenumber-js": "^1.7.52",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "oauth2-server": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "shelljs": "^0.8.3",
    "simple-command-bus": "^1.0.7",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5",
    "tv4": "^1.3.0",
    "typeorm": "^0.2.17",
    "uuid": "^3.3.2",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@sentry/node": "^5.7.0",
    "@types/ajv": "^1.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.17.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.15.18",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "express-mung": "^0.5.1",
    "handlebars": "^4.1.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-express-mock": "^2.2.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.9.6",
    "yo": "^2.0.6"
  }
}
